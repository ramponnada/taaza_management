nav.navbar.default-layout-navbar.col-lg-12.col-12.p-0.fixed-top.d-flex.flex-row
  .text-center.navbar-brand-wrapper.d-flex.align-items-center.justify-content-center
    a.navbar-brand.brand-logo(href='#')
      img(src='/uploads/general_images/taazapickleTitle.jpeg' alt='logo')
    a.navbar-brand.brand-logo-mini(href='#')
      img(src='/assets/images/logo-mini.svg' alt='logo')
  .navbar-menu-wrapper.d-flex.align-items-stretch
    button.navbar-toggler.navbar-toggler.align-self-center(type='button' data-toggle='minimize')
      span.mdi.mdi-menu
    .search-field.d-none.d-md-block
      form.d-flex.align-items-center.h-100(action='#')
        .input-group
          .input-group-prepend.bg-transparent
            i.input-group-text.border-0.mdi.mdi-magnify
          input.form-control.bg-transparent.border-0(type='text' placeholder='Search projects')
    ul.navbar-nav.navbar-nav-right
      li.nav-item.nav-profile.dropdown
        a#profileDropdown.nav-link.dropdown-toggle(href='#' data-toggle='dropdown' aria-expanded='false')
          .nav-profile-img
            img(src='/assets/images/faces/face1.jpg' alt='image')
            span.availability-status.online
          .nav-profile-text
            p.mb-1.text-black Admin
        .dropdown-menu.navbar-dropdown(aria-labelledby='profileDropdown')
          a.dropdown-item(href='#')
            i.mdi.mdi-cached.mr-2.text-success
            |  Activity Log
          .dropdown-divider
          a.dropdown-item(href="/authentication/logout")
            i.mdi.mdi-logout.mr-2.text-primary
            |  Signout
      li.nav-item.d-none.d-lg-block.full-screen-link
        a.nav-link
          i#fullscreen-button.mdi.mdi-fullscreen
      li.nav-item.dropdown
        a#messageDropdown.nav-link.count-indicator.dropdown-toggle(href='#' data-toggle='dropdown' aria-expanded='false')
          i.mdi.mdi-email-outline
          span.count-symbol.bg-warning
        .dropdown-menu.dropdown-menu-right.navbar-dropdown.preview-list(aria-labelledby='messageDropdown')
          h6.p-3.mb-0 Messages
          .dropdown-divider
          a.dropdown-item.preview-item
            .preview-thumbnail
              img.profile-pic(src='/assets/images/faces/face4.jpg' alt='image')
            .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
              h6.preview-subject.ellipsis.mb-1.font-weight-normal Mark send you a message
              p.text-gray.mb-0  1 Minutes ago 
          .dropdown-divider
          a.dropdown-item.preview-item
            .preview-thumbnail
              img.profile-pic(src='/assets/images/faces/face2.jpg' alt='image')
            .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
              h6.preview-subject.ellipsis.mb-1.font-weight-normal Cregh send you a message
              p.text-gray.mb-0  15 Minutes ago 
          .dropdown-divider
          a.dropdown-item.preview-item
            .preview-thumbnail
              img.profile-pic(src='/assets/images/faces/face3.jpg' alt='image')
            .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
              h6.preview-subject.ellipsis.mb-1.font-weight-normal Profile picture updated
              p.text-gray.mb-0  18 Minutes ago 
          .dropdown-divider
          h6.p-3.mb-0.text-center 4 new messages
      li.nav-item.dropdown
        a#notificationDropdown.nav-link.count-indicator.dropdown-toggle(href='#' data-toggle='dropdown')
          i.mdi.mdi-bell-outline
          span.count-symbol.bg-danger
        .dropdown-menu.dropdown-menu-right.navbar-dropdown.preview-list(aria-labelledby='notificationDropdown')
          h6.p-3.mb-0 Notifications
          .dropdown-divider
          a.dropdown-item.preview-item
            .preview-thumbnail
              .preview-icon.bg-success
                i.mdi.mdi-calendar
            .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
              h6.preview-subject.font-weight-normal.mb-1 Event today
              p.text-gray.ellipsis.mb-0  Just a reminder that you have an event today 
          .dropdown-divider
          a.dropdown-item.preview-item
            .preview-thumbnail
              .preview-icon.bg-warning
                i.mdi.mdi-settings
            .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
              h6.preview-subject.font-weight-normal.mb-1 Settings
              p.text-gray.ellipsis.mb-0  Update dashboard 
          .dropdown-divider
          a.dropdown-item.preview-item
            .preview-thumbnail
              .preview-icon.bg-info
                i.mdi.mdi-link-variant
            .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
              h6.preview-subject.font-weight-normal.mb-1 Launch Admin
              p.text-gray.ellipsis.mb-0  New admin wow! 
          .dropdown-divider
          h6.p-3.mb-0.text-center See all notifications
      li.nav-item.nav-logout.d-none.d-lg-block
        a.nav-link(href='#')
          i.mdi.mdi-power
      li.nav-item.nav-settings.d-none.d-lg-block
        a.nav-link(href='#')
          i.mdi.mdi-format-line-spacing
    button.navbar-toggler.navbar-toggler-right.d-lg-none.align-self-center(type='button' data-toggle='offcanvas')
      span.mdi.mdi-menu
